@config {
    @downlink_connections {
        dl_req_buffer_size: 8,
        buffer_size: 32,
        yield_after: 200,
        retry_strategy: @interval {
            delay: @duration {
                secs: 54,
                nanos: 0
            },
            retries: 13
        }
    },
    {
        back_pressure: @propagate
        idle_timeout: 60000
        buffer_size: 5
        on_invalid: @terminate
        yield_after: 256
    }
}