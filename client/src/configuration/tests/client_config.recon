@config {
    @client {
        buffer_size: 2
        router: {
            retry_strategy: @exponential(max_interval: 16, max_backoff: 300),
            idle_timeout: 60
            conn_reaper_frequency: 60
            buffer_size: 100
        }
    }
    @downlinks {
        back_pressure: @release(input_buffer_size: 5, bridge_buffer_size: 5, max_active_keys: 5, yield_after: 5),
        mux_mode: @buffered(queue_size: 5),
        idle_timeout: 60000
        buffer_size: 5
        on_invalid: "terminate"
        yield_after: 256
    }
    @host {
        "ws://127.0.0.1": {
            back_pressure: @propagate(),
            mux_mode: @queue(queue_size: 5),
            idle_timeout: 60000
            buffer_size: 5
            on_invalid: "terminate"
            yield_after: 256
        }
        "ws://127.0.0.2": {
            back_pressure: @propagate(),
            mux_mode: @dropping(),
            idle_timeout: 60000
            buffer_size: 5
            on_invalid: "terminate"
            yield_after: 256
        }
    }
    @lane {
        @path(host: "ws://127.0.0.1", node: "bar", lane: "baz"){
            back_pressure: @propagate(),
            mux_mode: @buffered(queue_size: 5),
            idle_timeout: 60000
            buffer_size: 5
            on_invalid: "terminate"
            yield_after: 256
        }
        @path(host: "ws://127.0.0.1", node: "qux", lane: "quz"){
            back_pressure: @propagate(),
            mux_mode: @buffered(queue_size: 5),
            idle_timeout: 60000
            buffer_size: 5
            on_invalid: "terminate"
            yield_after: 256
        }
    }
}